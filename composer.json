{
  "name": "contributte/crafter",
  "description": "Yummy opinionated PHP generator for web masters.",
  "keywords": [
    "crafter",
    "generator",
    "project",
    "assistant",
    "make"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/crafter",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "nette/di": "^3.2.2",
    "nette/utils": "^4.0.5",
    "nette/neon": "^3.4.3",
    "nette/schema": "^1.3.0",
    "nette/safe": "^1.0.0",
    "latte/latte": "^3.0.18",
    "symfony/console": "^7.1.5 || ^8.0.0",
    "psr/log": "^3.0.2",
    "laravel/prompts": "~0.3.2"
  },
  "require-dev": {
    "contributte/phpstan": "~0.3.0",
    "contributte/qa": "~0.4.0",
    "contributte/tester": "~0.4.0",
    "mockery/mockery": "^1.5.0",
    "humbug/box": "^4.6.2"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Crafter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "bin": [
    "bin/crafter"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
